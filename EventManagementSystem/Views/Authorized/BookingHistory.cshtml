@model IEnumerable<EventManagementSystem.Models.BookingEvent>

@{
    ViewBag.Title = "BookingHistory";
    Layout = "~/Views/Shared/AuthorizedLayout.cshtml";
}

<h2>BookingHistory</h2>

<h3>@ViewBag.Da</h3>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EventDetails.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Registration.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VenuDetails.VenuName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Food)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FoodType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Flower)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lighting)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SeatingService)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EventDetails.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Registration.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VenuDetails.VenuName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Food)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FoodType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Flower)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lighting)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SeatingService)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
       
    </tr>
    
}

</table>
